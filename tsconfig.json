{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ES2020",
      "lib": ["ES2020", "DOM", "DOM.Iterable"],
      "declaration": true,
      "declarationDir": "types",
      "outDir": "dist",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
  }